.PHONY	: all install clean tags dist userdoc devdoc
# DO NOT EDIT THIS FILE -- it was generated by configure.py
# configure.py --cuda-root=/usr/local/cuda-10.2

all: tags
	/home/tre/myenv/bin/python3 setup.py build

dist:
	/home/tre/myenv/bin/python3 setup.py sdist

install: tags
	/home/tre/myenv/bin/python3 setup.py install

clean:
	rm -Rf build
	rm -f tags

tags:
	ctags -R src || true

tests:
	echo "running tests"
	find ./test -type f -name "*.py" -exec python {} \;
